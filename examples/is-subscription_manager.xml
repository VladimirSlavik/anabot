<initial_setup>
    <subscription_manager>
        <server_panel>
            <server value="blablabla" />
            <default_server />
            <server value="SUBSCRIPTION_SERVER" />
            <proxy>
                <use_proxy checked="no" />
                <save />
            </proxy>
            <proxy>
                <use_proxy checked="yes" />
                <proxy_server value="172.30.32.3" />
                <test_connection />
                <save />
            </proxy>
            <next />
        </server_panel>
        <account_panel>
            <login value="LOGIN" />
            <password value="PASSWORD" />
            <system_name value="some.system.localdomain" />
            <register />
        </account_panel>
        <sla_panel policy="may_fail">
            <sla value="Self-Support" />
            <next />
        </sla_panel>
        <subscription_panel>
            <subscriptions />
            <attach />
        </subscription_panel>
        <done />
    </subscription_manager>
</initial_setup>
